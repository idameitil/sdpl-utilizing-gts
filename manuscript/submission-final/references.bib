@article{ganaie_structural_2021,
	title = {Structural, {Genetic}, and {Serological} {Elucidation} of {Streptococcus} pneumoniae {Serogroup} 24 {Serotypes}: {Discovery} of a {New} {Serotype}, {24C}, with a {Variable} {Capsule} {Structure}},
	volume = {59},
	issn = {0095-1137, 1098-660X},
	shorttitle = {Structural, {Genetic}, and {Serological} {Elucidation} of {Streptococcus} pneumoniae {Serogroup} 24 {Serotypes}},
	url = {https://journals.asm.org/doi/10.1128/JCM.00540-21},
	doi = {10.1128/JCM.00540-21},
	abstract = {Pneumococcal capsules are important in pneumococcal pathogenesis and vaccine development. Although conjugate vaccines have brought about a significant reduction in invasive pneumococcal disease (IPD) caused by vaccine serotypes, the relative serotype prevalence has shifted with the dramatic emergence of serotype 24F in some countries. Here, we describe 14 isolates (13 IPD and 1 non-IPD) expressing a new capsule type, 24C, which resembles 24F but has a novel serological proﬁle. We also describe the antigenic, biochemical, and genetic basis of 24F and 24C and the related serotypes 24A and 24B. Structural studies show that 24B, 24C, and 24F have identical polysaccharide backbones [b-Ribf-(1!4)-a-Rhap-(1!3)b-GlcpNAc-(1!4)-b-Rhap-(1!4)-b-Glcp] but with different side chains, as follows: 24F has arabinitol-phosphate and 24B has ribitol-phosphate. 24C has a mixture of 24F and 24B repeating units, with the ratio of ribitol to arabinitol being strain dependent. In contrast, the 24A capsule has a backbone without b-Ribf but with arabinitol-phosphate and phosphocholine side chains. These structures indicate that factor-sera 24d and 24e recognize arabinitol and ribitol, respectively, which explains the serology of serogroup 24, including those of 24C. The structures can be genetically described by the bispeciﬁcity of wcxG, which is capable of transferring arabinitol or ribitol when arabinitol is limiting. Arabinitol is likely not produced in 24B but is produced in reduced amounts in 24C due to various mutations in abpA or abpB genes. Our ﬁndings demonstrate how pneumococci modulate their capsule structure and immunologic properties with small genetic changes, thereby evading host immune responses. Our ﬁndings also suggest a potential for new capsule types within serogroup 24.},
	language = {en},
	number = {7},
	urldate = {2022-11-09},
	journal = {Journal of Clinical Microbiology},
	author = {Ganaie, Feroze and Maruhn, Karsten and Li, Chengxin and Porambo, Richard J. and Elverdal, Pernille L. and Abeygunwardana, Chitrananda and van der Linden, Mark and Duus, Jens Ø. and Sheppard, Carmen L. and Nahm, Moon H.},
	editor = {Richter, Sandra S.},
	month = jun,
	year = {2021},
	keywords = {notion},
	pages = {e00540--21},
}

